/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in= sel, out= Nots );
    And(a= a, b= Nots, out= aAndNotsel);
    And(a=b, b= sel, out = aAndSel);
    Or(a = aAndNotsel, b = aAndSel, out = out);
    
}